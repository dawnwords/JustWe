<REP name="userlogin">
    <RESULT>
        <RETCODE>返回码</RETCODE>
            >0成功即SessionID, 其他为失败，错误描述在MESSAGE 中
        <MESSAGE>返回的提示消息内容</MESSAGE>
        <MODULE_ID>模块ID</MODULE_ID>
        <LAST_TIME>上次登录时间</LAST_TIME>
        <LAST_IP>上次登录IP</LAST_IP>
        <CHG_PWD>是否需要修改密码；1：是；2：否；</CHG_PWD>
        <NAME>登录用户名称</NAME>
    </RESULT>
</REP>

<REP name="userlogoff">
    <RESULT>
        <RETCODE>返回码</RETCODE>
            =0成功, 其他为失败，错误描述在MESSAGE 中
        <MESSAGE>返回的提示消息内容</MESSAGE>    
    </RESULT>
</REP>

<REP name="check_user">
    <RESULT>
        <RETCODE>返回码</RETCODE>
            =0成功, 其他为失败，错误描述在MESSAGE 中
        <MESSAGE>返回的提示消息内容</MESSAGE>    
        <MODULE_ID>目标模块ID</MODULE_ID>
    </RESULT>
</REP>

<REP name="change_password">
    <RESULT>
        <RETCODE>返回码</RETCODE>
            =0成功, 其他为失败，错误描述在MESSAGE 中
        <MESSAGE>返回的提示消息内容</MESSAGE>    
    </RESULT>
</REP>

<REP name="firm_info">
    <RESULT>
        <RETCODE>返回码</RETCODE>
            =0成功, 其他为失败，错误描述在MESSAGE 中
        <MESSAGE>返回的提示消息内容</MESSAGE>    
    </RESULT>
    <RESULTLIST>            
        <REC>
        // 说明：为了兼容原来的远期和代理，把原来的协议做了个并集，
        //所以新版程序要给那些相同名称但标签名不一致的都要赋值，也
        //即标签颜色是粉红色的
            <FI>交易商ID</FI>    
            <FN>交易商名称</FN>  
            <IF>期初余额</IF>   
            <CM>上日保证金</CM>  
            <CF>上日浮亏</CF>       
            <RM>当日保证金</RM>  
            <RF>当日浮亏</RF>   
            <OR_F>远期委托冻结资金</OR_F>   
            <OT_F>其它冻结资金</OT_F> 
            <UF>当前可用资金</UF> 
            <FEE>当日手续费</FEE>    
            <PL>当日转让盈亏</PL> 
            //交易代码标签，为了兼容老的协议，保留<CDLST>，新增一个//<CDRS>标签,都需要赋值
            <CDLST>
                <CD>交易代码</CD>
            </CDLST>    
            <CDRS>
                <CDS>
                    <CD>交易代码</CD>
                </CDS>
            </CDRS> 
            <EQT>当前权益</EQT> 
            <IOF>出入金</IOF>  
            <HPF>订货盈亏</HPF> 
            <OC>其它变化</OC>   
            <PGF>质押资金</PGF> 
        </REC>
    </RESULTLIST>
</REP>

