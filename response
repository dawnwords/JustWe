<REP name="querytrade">
            <RESULT>
                <RETCODE>返回码</RETCODE>
                    =0成功, 其他为失败，错误描述在MESSAGE 中
                <MESSAGE>返回的提示消息内容</MESSAGE>
            </RESULT>
    <RESULTLIST>            
        <REC>
            <TR_N>成交号</TR_N>
            <OR_N>委托单号</OR_N>
            <TI>成交时间</TI>
            <TY>买卖标志：1买；2卖</TY>
            <SE_F>委托类型：1开仓；2平仓</SE_F>
            <TR_I>交易员ID</TR_I>
            <FI_I>交易商ID</FI_I>
            <CU_I>二级客户代码</CU_I>
            <CO_I>商品统一代码</CO_I>
            <PR>成交价格</PR>
            <QTY>成交数量</QTY>
            <O_PR>转让价</O_PR>
            <LIQPL>平仓盈亏</LIQPL>
            <COMM>交易手续费</COMM>
            <S_TR_N>被平仓成交单号</S_TR_N>
            <A_TR_N>交易市场或撮合成交号 </A_TR_N>    
            <TR_T>
                成交类型：
                1正常交易（开仓，平仓）
                2代理系统强平
                3交易市场强平
                4委托交易（开仓，平仓）
                5无委托成交
                6协议平仓
            </TR_T> 
            <TIMEFLAG>
                平仓类型：
                1：平今仓；
                2：平历史仓
                开仓时此节点为空
            </TIMEFLAG>
        </REC>
    </RESULTLIST>
</REP>

<REP name="sys_time_query">
    <RESULT>
        <RETCODE>返回码</RETCODE>
            =0成功, 其他为失败，错误描述在MESSAGE 中
        <MESSAGE>返回的提示消息内容</MESSAGE>    
        <CU_T>当前时间</CU_T>   
        <CU_D>当前日期</CU_D>   
        <TV_U>微妙</TV_U> 
        <L_ID>最后广播ID(空表示无消息)</L_ID>
        <CON>广播消息内容</CON>
        <BCRS>
        <BCS>
        <BC>广播消息内容</BC>
        </BCS>
        </BCRS>
        <NEW_T>
            是否有新成交：
            0:没有
            1：有
        </NEW_T>
        <TD_TTL>成交记录总数</TD_TTL>
        <TDRP>
            <TDS>
            <OR_N>委托单号</OR_N>
            <CO_I>商品统一代码</CO_I>
            <TDQTY>当前成交数量</TDQTY>
            </TDS>
        </TDRP>
        <DAY>交易日(格式: dd )</DAY>
    </RESULT>
</REP>

<REP name="my_order_query">
            <RESULT>
                <RETCODE>返回码</RETCODE>
                    =0成功, 其他为失败，错误描述在MESSAGE 中
                <MESSAGE>返回的提示消息内容</MESSAGE>    
        <TTLREC>总记录数</TTLREC>   
        <UT>最大更新时间, 微妙表示</UT>
                    </RESULT>
    <RESULTLIST>    
        <REC>
            <OR_N>委托单号</OR_N>   
            <TIME>下单时间</TIME>   
            <STA>状态：
                1:已委托
                2:部分成交
                3:全部成交
                5:全部撤单
                6:部分成交后撤单
            </STA>  
            <TYPE>买卖标志：1买；2卖</TYPE> 
            <SE_F>委托类型：1开仓；2平仓</SE_F>   
            <TR_I>交易员ID</TR_I>  
            <FI_I>交易商ID</FI_I>  
            <CU_I>二级客户代码 </CU_I>    
            <CO_I>商品统一代码</CO_I> 
            <PRI>委托价格</PRI> 
            <QTY>委托数量</QTY> 
            <BAL>剩余量</BAL>  
            <L_P>指定平仓价</L_P>    
            <WD_T>撤单时间</WD_T>   
            <C_F>平仓标志
                0：正常平仓
                1：代为转让
                2：强制转让
            </C_F>  
            <B_T_T>仓单交易类型
                0：正常（默认）
                1：卖仓单
                2：抵顶转让
            </B_T_T>
            <TIMEFLAG>平仓类型：
                0：不指定平仓
                1：平今仓
                2：平历史仓
                开仓时此节点为空
            </TIMEFLAG>
        </REC>
    </RESULTLIST>
</REP>

