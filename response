<REP name="commodity_data_query">
    <RESULT>
        <RETCODE>返回码</RETCODE>
            =0成功, 其他为失败，错误描述在MESSAGE 中
        <MESSAGE>返回的提示消息内容</MESSAGE>    
    </RESULT>
